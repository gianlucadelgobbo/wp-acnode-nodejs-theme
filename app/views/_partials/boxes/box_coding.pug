.boxItem(itemprop="itemListElement", itemscope, itemtype="http://schema.org/ListItem")
  meta(itemprop="position", content=position+1)
  .Web(itemscope, itemprop="item", itemtype='http://schema.org/CreativeWork')
    //.thumb-cnt
      //.catlabel=result.type
      a.ajaxloader(href=lang_preurl+'/coding/'+result.slug+"/", rel='bookmark', title!=result.post_title)
        img.img-fluid(src=result.featured.thumbnail, alt!=global.__('Image for')+': '+result.post_title, itemprop='image')
    .box-list
      h3.list_title(itemprop='name')
        a.ajaxloader(itemprop='url', href=lang_preurl+'/coding/'+result.slug+"/", rel='bookmark', title=result.post_title)!=result.post_title
      include ../../_partials/inc_datemonth
