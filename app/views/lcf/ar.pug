html
  head
    meta(name='viewport' content='width=device-width, initial-scale=1')
    script(src='https://cdn.jsdelivr.net/gh/hiukim/mind-ar-js@1.1.5/dist/mindar-image.prod.js')
    script(src='https://aframe.io/releases/1.2.0/aframe.min.js')
    script(src='https://cdn.jsdelivr.net/gh/donmccurdy/aframe-extras@v6.1.1/dist/aframe-extras.min.js')
    script(src='https://cdn.jsdelivr.net/gh/hiukim/mind-ar-js@1.1.5/dist/mindar-image-aframe.prod.js')
  body
    a-scene(mindar-image='imageTargetSrc: ' + config.prefix+'/ar/2022/LCF2022-promo-1.mind;' color-space='sRGB' renderer='colorManagement: true, physicallyCorrectLights' vr-mode-ui='enabled: false' device-orientation-permission-ui='enabled: false')
      a-assets
        video#bearModel(src=config.prefix+'/ar/2022/LCF2022-promo-1.mind.mp4')
      a-camera(position='0 0 0' look-controls='enabled: false')
      a-entity(mindar-image-target='targetIndex: 1')
        a-gltf-model(rotation='0 0 0 ' position='0 -0.25 0' scale='0.05 0.05 0.05' src='#bearModel' animation-mixer='')
